{
    "name": "herrera-io/silex-wise",
    "description": "A Silex service provider for Wise.",
    "keywords": ["silex", "config"],
    "homepage": "http://github.com/herrera-io/php-silex-wise",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        }
    ],
    "support": {
        "issues": "https://github.com/herrera-io/php-silex-wise/issues"
    },
    "require": {
        "php": ">=5.3.3",
        "herrera-io/wise": "~1.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "herrera-io/json": "~1.0",
        "herrera-io/phpunit-test-case": "1.*",
        "monolog/monolog": "1.*",
        "silex/silex": "~1.0",
        "symfony/yaml": "~2.1",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Herrera\\Wise": "src/lib"
        }
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
