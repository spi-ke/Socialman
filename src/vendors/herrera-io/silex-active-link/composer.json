{
    "name": "herrera-io/silex-active-link",
    "description": "A Silex service provider for finding an active link in Twig.",
    "keywords": ["silex", "twig"],
    "homepage": "http://github.com/herrera-io/php-silex-active-link",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        }
    ],
    "support": {
        "issues": "https://github.com/herrera-io/php-silex-active-link/issues"
    },
    "require": {
        "php": ">=5.3.3",
        "silex/silex": "~1.0",
        "twig/twig": "~1.0"
    },
    "require-dev": {
        "herrera-io/phpunit-test-case": "~1.0",
        "phpunit/phpunit": "~3.7",
        "symfony/twig-bridge": "~2.3"
    },
    "autoload": {
        "psr-0": {
            "Herrera\\Silex": "src/lib"
        }
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
